{
  "name": "angular-webpack-starter",
  "version": "0.1.1",
  "description": "Starter App with Angular Webpack and Karma",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./public",
    "prebuild": "npm run clean && mkdir public",
    "build": "cross-env NODE_ENV=production webpack --display-chunks --progress",
    "server": "cross-env NODE_ENV=development nodemon -w 'server/**/*.*' ./server/main.js",
    "server:dev": "cross-env NODE_ENV=development webpack-dev-server",
    "start": "npm run server:dev",
    "test": "cross-env NODE_ENV=test karma start --single-run",
    "deploy:firebase": "npm run build && firbase deploy"
  },
  "keywords": [
    "firebase",
    "angular"
  ],
  "author": "Sambhav Gore",
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^4.2.4",
    "@angular/common": "^4.2.4",
    "@angular/compiler": "^4.2.4",
    "@angular/core": "^4.2.4",
    "@angular/forms": "^4.2.4",
    "@angular/http": "^4.2.4",
    "@angular/material": "^2.0.0-beta.7",
    "@angular/platform-browser": "^4.2.4",
    "@angular/platform-browser-dynamic": "^4.2.4",
    "@angular/router": "^4.2.4",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "ng-redux": "^3.4.0-beta.1",
    "rxjs": "^5.4.1",
    "zone.js": "^0.8.12"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.41",
    "@types/firebase": "^2.4.31",
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.2",
    "autoprefixer": "^7.1.1",
    "awesome-typescript-loader": "^3.1.3",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.29.0",
    "jasmine-core": "^2.6.4",
    "jasmine-sinon": "^0.4.0",
    "karma": "^1.7.0",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-phantomjs-shim": "^1.4.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "morgan": "^1.8.2",
    "node-sass": "^4.5.3",
    "nodemon": "^1.11.0",
    "postcss-loader": "^2.0.6",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.6.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "typescript": "^2.3.4",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-md5-hash": "0.0.5",
    "winston": "^2.3.1"
  }
}
